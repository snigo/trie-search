{
  "name": "@gigwork/trie-search",
  "version": "0.1.0",
  "description": "Trie based search/filter library",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "rimraf dist && tsup",
    "format": "prettier --write src",
    "lint": "tsc --noEmit && eslint src",
    "lint:fix": "tsc --noEmit && eslint src --fix",
    "lint:staged": "lint-staged",
    "test": "glob -c \"node --import tsx --test\" \"src/**/*.test.ts\"",
    "prepare": "husky"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "author": "Igor Snitkin <sudo@snigo.dev>",
  "license": "MIT",
  "devDependencies": {
    "@gigwork/eslint-config-service": "^1.0.3",
    "@types/node": "^20.12.8",
    "eslint": "^9.2.0",
    "glob": "^11.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "rimraf": "^6.0.0",
    "tslib": "^2.6.2",
    "tsup": "^8.0.2",
    "tsx": "^4.9.3",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@gigwork/ds": "^0.3.3"
  }
}
